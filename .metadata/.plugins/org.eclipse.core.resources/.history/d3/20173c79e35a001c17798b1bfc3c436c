
public class Process {
	int arrivalTime;
	int burstTime;
	int name;
	
	public Process(int name, int burstTime) {
		this.name = name;
		this.burstTime = burstTime;
		this.arrivalTime = 0;
	}
	
	public Process(int name, int burstTime, int arrivalTime) {
		this.name = name;
		this.burstTime = burstTime;
		this.arrivalTime = arrivalTime;
	}
	
	
}
